let age=prompt("enter your age:")
if (age<0)
    console.log("enter valid age");
let gender=prompt("enter your gender(f/m):");
if(gender=='f'){
    if (age>=18)
        console.log("eligible for marriage");
    else
        console.log("not eligible for marriage");
}
else{
    if (age>=21)
        console.log("eligible for marriage");
    else
        console.log("not eligible for marriage");
}    
